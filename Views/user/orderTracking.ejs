            /* Smooth transitions */
        }

        .payment-heading:hover {
            box-shadow: 0 8px 12px rgba(0, 0, 0, 0.5);
            /* Deeper shadow on hover */
            transform: translateY(-2px);
            /* Slight lift on hover */
        }
        .message-container {
